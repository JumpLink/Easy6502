using Gtk 4.0;
using Adw 1;

template $Debugger: Adw.Bin {
  Box {
    orientation: vertical;

    Label {
      label: "Hex Monitor";
      halign: start;
      styles [
        "heading"
      ]
    }
    $HexMonitor hexMonitor {}

    Label {
      label: "Output";
      halign: start;
      styles [
        "heading"
      ]
    }
    $MessageConsole messageConsole {}
  }
}
