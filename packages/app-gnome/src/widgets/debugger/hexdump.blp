using Gtk 4.0;
using Adw 1;

template $Hexdump: Adw.Bin {
  $SourceView sourceView {
    editable: false;
    copyable: true;
    copy-button-icon: 'clipboard-symbolic';
    language: 'hex';
    vexpand: true;
    line-numbers: true;
    line-number-start: 0x0600;
    height: 60;
    fit-content-height: false;
  }
}
