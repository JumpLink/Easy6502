using Gtk 4.0;
using Adw 1;

template $Hexdump: Adw.Bin {
  $SourceView sourceView {
    editable: false;
    copyable: true;
    language: 'hex';
    vexpand: true;
    line-numbers: true;
    line-number-start: 0x0600;
    // TODO: Fix scrollable height
    height-request: 50;
    fit-content-height: true;
  }
}
